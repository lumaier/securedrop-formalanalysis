lemma Executability:
  exists-trace
  "∃ #t1 #t2 #t3 #t4 s_fetch_pk s_apke_pk j_apke_pk nr srs m1 sid
     j_eapke_pk j_fetching_pk m2 sss jrs jid s_fetching_pk jss.
    (((((((((Source_Received( s_apke_pk, s_fetch_pk, j_apke_pk, nr, m1
             ) @ #t1) ∧
