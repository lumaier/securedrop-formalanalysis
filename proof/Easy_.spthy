restriction PKE_Consistency:
  "∀ ct skR pt1 pt2 #t1 #t2.
    ((PKE_Dec( ct, skR, pt1 ) @ #t1) ∧ (PKE_Dec( ct, skR, pt2 ) @ #t2)) ⇒
    (pt1 = pt2)"
  // safety formula

restriction PKE_Correctness:
  "∀ pt skR mDec #t1 #t2.
    ((PKE_Dec( PKE_Enc(pk(skR), pt), skR, mDec ) @ #t1) ∧
     (Honest( pk(skR) ) @ #t2)) ⇒
    (mDec = pt)"
  // safety formula

